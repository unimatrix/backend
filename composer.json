{
    "name": "unimatrix/backend",
    "description": "Backend for CakePHP 3.6",
    "type": "cakephp-plugin",
    "homepage": "https://venture.unimatrix.ro",
    "license": "MIT",
    "require": {
        "unimatrix/cake": "^3.0",
        "unimatrix/ckfinder": "^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7.14|^6.0",
        "symfony/var-dumper": "~2.7|~3.0|~4.0"
    },
    "autoload": {
        "psr-4": {
            "Unimatrix\\Backend\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Unimatrix\\Backend\\Test\\": "tests"
        }
    }
}